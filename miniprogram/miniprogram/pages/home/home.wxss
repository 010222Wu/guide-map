/* pages/home/home.wxss */

.status-bar {
  background: #1c659c;
}

.head {
  display: flex;
  padding-left: 32rpx;
  align-items: center;
  background-color: #1c659c;
  background-size: 100% 100%;
  -moz-background-size: 100% 100%;
  background-image:url('data:image/png;base64,');
}



.head-item{
  color: #ffffff;
  font-size: 37rpx;
}

.topsign{
  position: fixed;
  width: 100%;
  z-index: -1;
}

.page-body {
  width: 100%;
}

.menu {
  /* 如果要实现滑动则要添加下面这两行代码 */
  overflow: hidden;
  white-space: nowrap;
  background: linear-gradient(45deg, #0081ff, #1cbbb4);
  /* background: #1c659c; */
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-items: center;
}

/* 隐藏滚动条 */

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.menu-item {
  /* 如果要实现滑动则要添加下面这行代码 */
  display: inline-flex;
  align-items: center;
  position: relative;
  /* margin: 0 15rpx 0 15rpx; */
  font-size: 30rpx;
  color: #fff;
  padding: 0 30rpx 0 30rpx;
}

.menu-item-select {
  background-image: url('data:image/png;base64,');
  background-size: 100% 100%;
  -moz-background-size: 100% 100%;
}

.map {
  height: 100%;
  width: 100%;
}

.hint {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
  color: #1c659c;
  border-bottom: 1px solid #beb6b6;
}

.arr{
  margin-left: 10rpx;
  width: 35rpx;
  height: 20rpx;
}

.list-item {
  font-size: 35rpx;
	text-align: center;
  color: #000000;
  padding: 0 40rpx 0 40rpx;
  padding-bottom: 20rpx;
  padding-top: 20rpx;
  /* border-bottom: 1rpx solid #beb6b6; */
  display: flex;
  justify-content: space-between;
}

.bgdc {
  background-color: rgb(197, 225, 241);
}

.navigate-text {
	padding: 10rpx;
  color: #1c659c;
}

.navigate-btn {
	padding: 10rpx;
  width:49rpx;
  height:42rpx;
  opacity: 0.9;
}

.btn {
  position: absolute;
  top: 77%;
  left: 83%;
}

.locate-img {
  width: 68rpx;
  height: 68rpx;
  position: absolute;
  bottom: 50rpx;
  right: 50rpx;
}

.list-about-us {
	display: flex;
  justify-content: center;
  font-size: 28rpx;
  margin-top: 20rpx;
  color: #D5D5D5;
}

.about-us {
	display: flex;
	flex-direction: column;
	align-items: center;
  color: #1c659c;
}

.about-us-title{
  display: flex;
  flex-direction: row;
  align-items: center;
}

.about-us-img {
  margin-left: 10rpx;
  width: 35rpx;
  height: 35rpx;
}

.about-us-head {
	margin-top: 30rpx;
	opacity: 0;
}

.about-us-head1 {
	opacity: 0;
}

.about-us-body {
	margin: 30rpx;
	opacity: 0;
}

